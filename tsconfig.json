{
  "compilerOptions": {
    "jsx": "react",
    "target": "ESNext",
    "noImplicitAny": true,
    "module": "ESNext",
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "baseUrl": "src",
    "allowSyntheticDefaultImports":true,
    "moduleResolution": "Node"
  },
  "include": ["src/**/*"],
  "exclude": ["**/*.test.ts"]
}